# DISCOVERY_QUERY_COOLDOWN_MINS=60

# Agent Configuration Otaku

# =============================================================================
# CORE CONFIGURATION (REQUIRED)
# =============================================================================

# JWT secret for user authentication (generate with: openssl rand -base64 32)
JWT_SECRET="your-random-32-byte-base64-string"

# Gamification (Rebels ranking, points, referral link). Base URL for invite links; default https://otaku.so. No trailing slash.
# GAMIFICATION_REFERRAL_BASE_URL=https://otaku.so

# AI Model API Keys (at least one required)
OPENAI_API_KEY="sk-proj-..."
OPENROUTER_API_KEY="sk-or-v1-..."

# OpenRouter model selection (optional, defaults to GPT-4)
OPENROUTER_SMALL_MODEL="moonshotai/kimi-k2-0905"
OPENROUTER_LARGE_MODEL="moonshotai/kimi-k2-0905"

# If you see "402 - This request requires more credits, or fewer max_tokens" from OpenRouter:
# - Add credits at https://openrouter.ai/settings/keys (recommended), or
# - Use a cheaper/smaller model above (e.g. google/gemini-2.0-flash-001) to use fewer credits per request, or
# - Rely on another provider by setting OPENAI_API_KEY / ANTHROPIC_API_KEY so that provider handles text generation when OpenRouter is low.
# Note: Otaku does not use OpenRouter; Otaku uses the OpenAI plugin when OPENAI_API_KEY is set. If Otaku shows a 402, it may refer to another agent or an old config.

# =============================================================================
# COINBASE DEVELOPER PLATFORM (CDP) WALLET
# =============================================================================

# CDP Project ID for frontend wallet integration
# Get from: https://portal.cdp.coinbase.com/
VITE_CDP_PROJECT_ID="your-cdp-project-id"

# CDP API credentials for backend SDK
CDP_API_KEY_ID="your-cdp-api-key-id"
CDP_API_KEY_SECRET="your-cdp-api-key-secret"

# CDP wallet encryption secret (generate with: openssl rand -hex 32)
CDP_WALLET_SECRET="your-64-character-hex-string"

# =============================================================================
# BLOCKCHAIN RPC ENDPOINTS
# =============================================================================

# Alchemy API key for balance/token/NFT fetching
# Get from: https://dashboard.alchemy.com/
ALCHEMY_API_KEY="your-alchemy-api-key"

# RPC URLs (using Alchemy endpoints)
BASE_RPC_URL="https://base-mainnet.g.alchemy.com/v2/your-alchemy-api-key"
ETHEREUM_RPC_URL="https://eth-mainnet.g.alchemy.com/v2/your-alchemy-api-key"
POLYGON_RPC_URL="https://polygon-mainnet.g.alchemy.com/v2/your-alchemy-api-key"
ARBITRUM_RPC_URL="https://arb-mainnet.g.alchemy.com/v2/your-alchemy-api-key"
OPTIMISM_RPC_URL="https://opt-mainnet.g.alchemy.com/v2/your-alchemy-api-key"
SCROLL_RPC_URL="https://scroll-mainnet.g.alchemy.com/v2/your-alchemy-api-key"

# =============================================================================
# X402 PAYMENT PROTOCOL (OTAKU)
# =============================================================================
# HTTP-native crypto payments via x402.org standard
# Otaku (COO) is the wallet agent — receives USDC payments on Base

# Enable/disable x402 payments (default: false)
X402_ENABLED=false

# Wallet private key for payment signing (hex, 0x-prefixed)
# ⚠️ SENSITIVE - never commit this
# X402_PRIVATE_KEY="0x..."

# Network for x402 payments
# Options: base (mainnet), base-sepolia (testnet)
X402_NETWORK="base-sepolia"

# Wallet address to receive USDC payments
X402_PAY_TO="0xYourWalletAddress"

# Public URL clients use to access the API (required in production)
X402_PUBLIC_URL="https://yourdomain.com"

# X402 facilitator URL for payment verification
# Mainnet: https://x402.coinbase.com (default)
# Testnet: https://x402.org/facilitator
X402_FACILITATOR_URL="https://x402.org/facilitator"

# Payment limits (USD) - circuit breaker protection
X402_MAX_PAYMENT_USD=10
X402_MAX_TOTAL_USD=1000

# Route pricing (in USDC base units, 6 decimals)
# $0.01 = 10000, $0.05 = 50000, $0.10 = 100000, $1.00 = 1000000
X402_PRICE_POSITIONS=50000    # /otaku/positions - $0.05
X402_PRICE_QUOTE=20000        # /otaku/quote - $0.02
X402_PRICE_YIELDS=100000      # /otaku/yields - $0.10
X402_PRICE_HISTORY=50000      # /otaku/history - $0.05
X402_PRICE_PORTFOLIO=250000   # /otaku/portfolio - $0.25

# =============================================================================
# PLUGIN API KEYS (OPTIONAL)
# =============================================================================

# Web search plugin (required if plugin-web-search enabled)
# Get from: https://tavily.com/
TAVILY_API_KEY="tvly-..."

# Token pricing and market data
# Get from: https://www.coingecko.com/en/api
# Free tier is rate-limited; without a key the plugin uses the public API. If the coins index fails at startup (e.g. 429), the plugin still works for direct price calls (e.g. getSimplePrices / simple/price).
COINGECKO_API_KEY="CG-..."
# For Demo API keys: set to true so the plugin uses api.coingecko.com instead of pro-api. Public API is used automatically when no key is set.
# COINGECKO_USE_PUBLIC_URL=true

# 0x API for token swaps
# Get from: https://0x.org/docs/api
OX_API_KEY="your-0x-api-key"

# CoinDesk news API
# Get from: https://developer.coindesk.com/
COINDESK_API_KEY="your-coindesk-api-key"

# Etherscan transaction verification
# Get from: https://etherscan.io/apis
ETHERSCAN_API_KEY="your-etherscan-api-key"

# Web scraping service
# Get from: https://firecrawl.dev/
FIRECRAWL_API_KEY="fc-..."

# Nansen blockchain analytics
# Get from: https://www.nansen.ai/
NANSEN_API_KEY="your-nansen-api-key"

# =============================================================================
# SERVER CONFIGURATION (OPTIONAL)
# =============================================================================

# Server port (default: 3000)
# SERVER_PORT=3000

# Logging level (default: info)
# Options: error, warn, info, debug
LOG_LEVEL="debug"

# Node environment
# Options: development, production
NODE_ENV="production"

# =============================================================================
# DATABASE (OPTIONAL)
# =============================================================================

# PostgreSQL connection URL (overrides default PGlite)
# Format: postgres://user:password@host:port/database
POSTGRES_URL="postgres://user:password@host:port/database"

# PGlite data directory (default: ./data)
# PGLITE_DATA_DIR="./data"

DAILY_STANDUP_ENABLED=true
STANDUP_COORDINATOR=Kelly

A2A_ENABLED=true
A2A_MAX_EXCHANGES=3
A2A_LOOKBACK_MESSAGES=10